<label for="title">Title:</label>
<input type="text" name="song[title]" id="title" value=<%= @song.title %>><br>
<lable for="length">Length:</lable>
<input type="number" name="song[length]" id="length" value=<% @song.length %>>
<lable for="released_on">Date(mm/dd/yyyy):</lable>
<input type="text" name="song[released_on]" id="released_on" value=<% @song.released_on if @song.released_on %>>
<lable for="lyrics">Lyrics:</lable>
<textarea name="song[lyrics]" id="lyrics" value=<%= @song.lyrics %>></textarea>
<br>
<br>
<input type="submit" value="save song">